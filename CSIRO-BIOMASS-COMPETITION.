   "source": [
    "import os\n",
    "import gc\n",
    "import math\n",
    "import json\n",
    "import random\n",
    "import argparse\n",
    "import shutil\n",
    "import time\n",
    "from copy import deepcopy\n",
    "from dataclasses import dataclass\n",
    "from typing import Optional, List, Tuple, Dict\n",
    "from pathlib import Path\n",
    "\n",
    "import cv2\n",
    "import timm\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "import albumentations as A\n",
    "import torch\n",
    "import torch.nn as nn\n",
    "import torch.nn.functional as F\n",
    "from torch.utils.data import Dataset, DataLoader\n",
    "from albumentations.pytorch import ToTensorV2\n",
    "from PIL import Image\n",
    "from tqdm.auto import tqdm\n",
    "\n",
    "from sklearn.model_selection import KFold\n",
    "from sklearn.linear_model import LinearRegression, Ridge, Lasso, BayesianRidge\n",
    "from sklearn.svm import SVR, LinearSVR\n",
    "from sklearn.ensemble import GradientBoostingRegressor, HistGradientBoostingRegressor, ExtraTreesRegressor, RandomForestRegressor\n",
    "from sklearn.dummy import DummyRegressor\n",
    "from sklearn.decomposition import PCA\n",
    "from sklearn.cross_decomposition import PLSRegression\n",
    "from sklearn.mixture import GaussianMixture\n",
    "from sklearn import preprocessing\n",
    "from sklearn.metrics.pairwise import cosine_similarity\n",
    "from sklearn.base import BaseEstimator, TransformerMixin\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "\n",
    "from xgboost import XGBRegressor\n",
    "from lightgbm import LGBMRegressor\n",
    "from catboost import CatBoostRegressor\n",
    "\n",
    "from transformers import AutoProcessor, AutoImageProcessor, AutoModel, AutoTokenizer\n",
    "\n",
    "# ============================================================================\n",
    "# TIMEOUT MANAGER (Enhanced)\n",
    "# ============================================================================\n",
    "class TimeoutManager:\n",
    "    \"\"\"Manages execution time to prevent timeout\"\"\"\n",
    "    def __init__(self, max_time_seconds):\n",
    "        self.start_time = time.time()\n",
    "        self.max_time = max_time_seconds\n",
    "        self.checkpoints = {}\n",
    "        \n",
    "    def time_elapsed(self):\n",
    "        return time.time() - self.start_time\n",
    "        \n",
    "    def time_remaining(self):\n",
    "        return self.max_time - self.time_elapsed()\n",
    "    \n",
    "    def should_continue(self, buffer_seconds=300):\n",
    "        \"\"\"Check if we have enough time to continue (with buffer)\"\"\"\n",
    "        return self.time_remaining() > buffer_seconds\n",
    "    \n",
    "    def log_time(self, message=\"\"):\n",
    "        elapsed = self.time_elapsed()\n",
    "        remaining = self.time_remaining()\n",
    "        print(f\"[TIME] {message} | Elapsed: {elapsed/3600:.2f}h | Remaining: {remaining/3600:.2f}h\")\n",
    "        self.checkpoints[message] = elapsed\n",
    "    \n",
    "    def get_summary(self):\n",
    "        \"\"\"Print time summary\"\"\"\n",
    "        print(\"\\n\" + \"=\"*80)\n",
    "        print(\"TIME SUMMARY\")\n",
    "        print(\"=\"*80)\n",
    "        for msg, elapsed in self.checkpoints.items():\n",
    "            print(f\"{msg}: {elapsed/60:.2f} minutes\")\n",
    "        print(\"=\"*80)\n",
    "\n",
    "# ============================================================================\n",
    "# UTILITY FUNCTIONS\n",
    "# ============================================================================\n",
    "def seeding(SEED):\n",
    "    np.random.seed(SEED)\n",
    "    random.seed(SEED)\n",
    "    os.environ['PYTHONHASHSEED'] = str(SEED)\n",
    "    torch.manual_seed(SEED)\n",
    "    if torch.cuda.is_available(): \n",
    "        torch.cuda.manual_seed(SEED)\n",
    "        torch.cuda.manual_seed_all(SEED)\n",
    "        torch.backends.cudnn.deterministic = True\n",
    "        torch.backends.cudnn.benchmark = False\n",
    "\n",
    "def flush():\n",
    "    gc.collect()\n",
    "    if torch.cuda.is_available():\n",
    "        torch.cuda.empty_cache()\n",
    "        torch.cuda.reset_peak_memory_stats()\n",
    "\n",
    "@dataclass\n",
    "class Config:\n",
    "    DATA_PATH: Path = Path(\"/kaggle/input/csiro-biomass/\")\n",
    "    TRAIN_DATA_PATH: Path = DATA_PATH/'train'\n",
    "    TEST_DATA_PATH: Path = DATA_PATH/'test'\n",
    "    device = \"cuda\" if torch.cuda.is_available() else \"cpu\"\n",
    "    seed = 42\n",
    "\n",
    "cfg = Config()\n",
    "seeding(cfg.seed)\n",
    "\n",
    "TARGET_NAMES = ['Dry_Clover_g', 'Dry_Dead_g', 'Dry_Green_g', 'Dry_Total_g', 'GDM_g']\n",
    "weights = {\n",
    "    'Dry_Green_g': 0.1,\n",
    "    'Dry_Dead_g': 0.1,\n",
    "    'Dry_Clover_g': 0.1,\n",
    "    'GDM_g': 0.2,\n",
    "    'Dry_Total_g': 0.5,\n",
    "}\n",
    "TARGET_MAX = {\n",
    "    \"Dry_Clover_g\": 71.7865,\n",
    "    \"Dry_Dead_g\": 83.8407,\n",
    "    \"Dry_Green_g\": 157.9836,\n",
    "    \"Dry_Total_g\": 185.70,\n",
    "    \"GDM_g\": 157.9836,\n",
    "}\n",
    "\n",
    "def competition_metric(y_true, y_pred) -> float:\n",
    "    y_weighted = 0\n",
    "    for l, label in enumerate(TARGET_NAMES):\n",
    "        y_weighted = y_weighted + y_true[:, l].mean() * weights[label]\n",
    "    ss_res = 0\n",
    "    ss_tot = 0\n",
    "    for l, label in enumerate(TARGET_NAMES):\n",
    "        ss_res = ss_res + ((y_true[:, l] - y_pred[:, l])**2).mean() * weights[label]\n",
    "        ss_tot = ss_tot + ((y_true[:, l] - y_weighted)**2).mean() * weights[label]\n",
    "    return 1 - ss_res / ss_tot\n",
    "\n",
    "def pivot_table(df: pd.DataFrame)->pd.DataFrame:\n",
    "    if 'target' in df.columns.tolist():\n",
    "        df_pt = pd.pivot_table(\n",
    "            df, \n",
    "            values='target', \n",
    "            index=['image_path', 'Sampling_Date', 'State', 'Species', 'Pre_GSHH_NDVI', 'Height_Ave_cm'], \n",
    "            columns='target_name', \n",
    "            aggfunc='mean'\n",
    "        ).reset_index()\n",
    "    else:\n",
    "        df['target'] = 0\n",
    "        df_pt = pd.pivot_table(\n",
    "            df, \n",
    "            values='target', \n",
    "            index='image_path', \n",
    "            columns='target_name', \n",
    "            aggfunc='mean'\n",
    "        ).reset_index()\n",
    "    return df_pt\n",
    "\n",
    "def melt_table(df: pd.DataFrame) -> pd.DataFrame:\n",
    "    melted = df.melt(\n",
    "        id_vars='image_path',\n",
    "        value_vars=TARGET_NAMES,\n",
    "        var_name='target_name',\n",
    "        value_name='target'\n",
    "    )\n",
    "    melted['sample_id'] = (\n",
    "        melted['image_path']\n",
    "        .str.replace(r'^.*/', '', regex=True)\n",
    "        .str.replace('.jpg', '', regex=False)\n",
    "        + '__' + melted['target_name']\n",
    "    )\n",
    "    return melted[['sample_id', 'image_path', 'target_name', 'target']]\n",
    "\n",
    "def post_process_biomass(df_preds, method='hierarchical'):\n",
    "    \"\"\"\n",
    "    Enhanced post-processing with multiple methods\n",
    "    \"\"\"\n",
    "    ordered_cols = [\"Dry_Green_g\", \"Dry_Clover_g\", \"Dry_Dead_g\", \"GDM_g\", \"Dry_Total_g\"]\n",
    "    Y = df_preds[ordered_cols].values.T\n",
    "    \n",
    "    if method == 'hierarchical':\n",
    "        # Hierarchical reconciliation\n",
    "        C = np.array([[1, 1, 0, -1,  0], [0, 0, 1,  1, -1]])\n",
    "        C_T = C.T\n",
    "        inv_CCt = np.linalg.inv(C @ C_T)\n",
    "        P = np.eye(5) - C_T @ inv_CCt @ C\n",
    "        Y_reconciled = P @ Y\n",
    "        Y_reconciled = Y_reconciled.T.clip(min=0)\n",
    "    \n",
    "    elif method == 'simple':\n",
    "        # Simple constraint satisfaction\n",
    "        Y_reconciled = Y.T.copy()\n",
    "        # Ensure GDM = Green + Clover\n",
    "        Y_reconciled[:, 3] = Y_reconciled[:, 0] + Y_reconciled[:, 1]\n",
    "        # Ensure Total = GDM + Dead\n",
    "        Y_reconciled[:, 4] = Y_reconciled[:, 3] + Y_reconciled[:, 2]\n",
    "        Y_reconciled = Y_reconciled.clip(min=0)\n",
    "    \n",
    "    elif method == 'weighted':\n",
    "        # Weighted averaging approach\n",
    "        Y_reconciled = Y.T.copy()\n",
    "        # For GDM: 70% predicted, 30% from sum\n",
    "        gdm_predicted = Y_reconciled[:, 3]\n",
    "        gdm_from_sum = Y_reconciled[:, 0] + Y_reconciled[:, 1]\n",
    "        Y_reconciled[:, 3] = 0.7 * gdm_predicted + 0.3 * gdm_from_sum\n",
    "        \n",
    "        # For Total: 70% predicted, 30% from sum\n",
    "        total_predicted = Y_reconciled[:, 4]\n",
    "        total_from_sum = Y_reconciled[:, 3] + Y_reconciled[:, 2]\n",
    "        Y_reconciled[:, 4] = 0.7 * total_predicted + 0.3 * total_from_sum\n",
    "        Y_reconciled = Y_reconciled.clip(min=0)\n",
    "    \n",
    "    else:\n",
    "        Y_reconciled = Y.T.clip(min=0)\n",
    "    \n",
    "    df_out = df_preds.copy()\n",
    "    df_out[ordered_cols] = Y_reconciled\n",
    "    return df_out\n",
    "\n",
    "def split_image(image, patch_size=520, overlap=16):\n",
    "    h, w, c = image.shape\n",
    "    stride = patch_size - overlap\n",
    "    patches, coords = [], []\n",
    "    for y in range(0, h, stride):\n",
    "        for x in range(0, w, stride):\n",
    "            y1, x1, y2, x2 = y, x, y + patch_size, x + patch_size\n",
    "            patch = image[y1:y2, x1:x2, :]\n",
    "            if patch.shape[0] < patch_size or patch.shape[1] < patch_size:\n",
    "                pad_h = patch_size - patch.shape[0]\n",
    "                pad_w = patch_size - patch.shape[1]\n",
    "                patch = np.pad(patch, ((0,pad_h), (0,pad_w), (0,0)), mode='reflect')\n",
    "            patches.append(patch)\n",
    "            coords.append((y1, x1, y2, x2))\n",
    "    return patches, coords\n",
    "\n",
    "def get_model(model_path: str, device: str = 'cpu'):\n",
    "    model = AutoModel.from_pretrained(model_path, local_files_only=True)\n",
    "    processor = AutoImageProcessor.from_pretrained(model_path)\n",
    "    return model.eval().to(device), processor\n",
    "\n",
    "def compute_embeddings(model_path, df, patch_size=520):\n",
    "    device = 'cuda' if torch.cuda.is_available() else 'cpu'\n",
    "    model, processor = get_model(model_path=model_path, device=device)\n",
    "    IMAGE_PATHS, EMBEDDINGS = [], []\n",
    "    for i, row in tqdm(df.iterrows(), total=len(df), desc=\"Computing embeddings\"):\n",
    "        img_path = row['image_path']\n",
    "        img = cv2.imread(img_path, cv2.IMREAD_COLOR)\n",
    "        patches, coords = split_image(img, patch_size=patch_size)\n",
    "        images = [Image.fromarray(p).convert(\"RGB\") for p in patches]\n",
    "        inputs = processor(images=images, return_tensors=\"pt\").to(model.device)\n",
    "        with torch.no_grad():\n",
    "            if 'siglip' in model_path:\n",
    "                features = model.get_image_features(**inputs)\n",
    "            elif 'dino' in model_path:\n",
    "                features = model(**inputs).pooler_output\n",
    "            else:\n",
    "                raise Exception(\"Model should be dino or siglip\")\n",
    "        embeds = features.mean(dim=0).detach().cpu().numpy()\n",
    "        EMBEDDINGS.append(embeds)\n",
    "        IMAGE_PATHS.append(img_path)\n",
    "    embeddings = np.stack(EMBEDDINGS, axis=0)\n",
    "    n_features = embeddings.shape[1]\n",
    "    emb_columns = [f\"emb{i+1}\" for i in range(n_features)]\n",
    "    emb_df = pd.DataFrame(embeddings, columns=emb_columns)\n",
    "    emb_df['image_path'] = IMAGE_PATHS\n",
    "    df_final = df.merge(emb_df, on='image_path', how='left')\n",
    "    flush()\n",
    "    return df_final\n",
    "\n",
    "def generate_semantic_features(image_embeddings, model_path):\n",
    "    device = 'cuda' if torch.cuda.is_available() else 'cpu'\n",
    "    try:\n",
    "        model = AutoModel.from_pretrained(model_path).to(device)\n",
    "        tokenizer = AutoTokenizer.from_pretrained(model_path)\n",
    "    except Exception as e:\n",
    "        return None\n",
    "    \n",
    "    # Enhanced concept groups with more diversity\n",
    "    concept_groups = {\n",
    "        \"bare\": [\"bare soil\", \"dirt ground\", \"sparse vegetation\", \"exposed earth\", \"no grass\"],\n",
    "        \"sparse\": [\"low density pasture\", \"thin grass\", \"short clipped grass\", \"minimal coverage\"],\n",
    "        \"medium\": [\"average pasture cover\", \"medium height grass\", \"grazed pasture\", \"moderate density\"],\n",
    "        \"dense\": [\"dense tall pasture\", \"thick grassy volume\", \"high biomass\", \"overgrown vegetation\", \"lush coverage\"],\n",
    "        \"green\": [\"lush green vibrant pasture\", \"photosynthesizing leaves\", \"fresh growth\", \"healthy vegetation\"],\n",
    "        \"dead\": [\"dry brown dead grass\", \"yellow straw\", \"senesced material\", \"standing hay\", \"dried vegetation\"],\n",
    "        \"clover\": [\"white clover\", \"trifolium repens\", \"broadleaf legume\", \"clover flowers\", \"nitrogen-fixing plants\"],\n",
    "        \"grass\": [\"ryegrass\", \"blade-like leaves\", \"fescue\", \"grassy sward\", \"monocot vegetation\"],\n",
    "        \"weeds\": [\"broadleaf weeds\", \"thistles\", \"non-pasture vegetation\", \"invasive species\"],\n",
    "        \"texture_fine\": [\"fine-textured\", \"small leaves\", \"delicate structure\"],\n",
    "        \"texture_coarse\": [\"coarse texture\", \"large leaves\", \"thick stems\"],\n",
    "        \"moisture_high\": [\"wet grass\", \"dew-covered\", \"high moisture content\"],\n",
    "        \"moisture_low\": [\"dry grass\", \"drought-stressed\", \"low moisture\"],\n",
    "    }\n",
    "    \n",
    "    concept_vectors = {}\n",
    "    with torch.no_grad():\n",
    "        for name, prompts in concept_groups.items():\n",
    "            inputs = tokenizer(prompts, padding=\"max_length\", return_tensors=\"pt\").to(device)\n",
    "            emb = model.get_text_features(**inputs)\n",
    "            emb = emb / emb.norm(p=2, dim=-1, keepdim=True)\n",
    "            concept_vectors[name] = emb.mean(dim=0, keepdim=True)\n",
    "    \n",
    "    if isinstance(image_embeddings, np.ndarray):\n",
    "        img_tensor = torch.tensor(image_embeddings, dtype=torch.float32).to(device)\n",
    "    else:\n",
    "        img_tensor = image_embeddings.to(device)\n",
    "    img_tensor = img_tensor / img_tensor.norm(p=2, dim=-1, keepdim=True)\n",
    "    \n",
    "    scores = {}\n",
    "    for name, vec in concept_vectors.items():\n",
    "        scores[name] = torch.matmul(img_tensor, vec.T).cpu().numpy().flatten()\n",
    "    \n",
    "    df_scores = pd.DataFrame(scores)\n",
    "    \n",
    "    # Enhanced derived features\n",
    "    df_scores['ratio_greenness'] = df_scores['green'] / (df_scores['green'] + df_scores['dead'] + 1e-6)\n",
    "    df_scores['ratio_clover'] = df_scores['clover'] / (df_scores['clover'] + df_scores['grass'] + 1e-6)\n",
    "    df_scores['ratio_cover'] = (df_scores['dense'] + df_scores['medium']) / (df_scores['bare'] + df_scores['sparse'] + 1e-6)\n",
    "    df_scores['max_density'] = df_scores[['bare', 'sparse', 'medium', 'dense']].max(axis=1)\n",
    "    df_scores['density_variance'] = df_scores[['bare', 'sparse', 'medium', 'dense']].std(axis=1)\n",
    "    df_scores['biomass_indicator'] = df_scores['dense'] * df_scores['green']\n",
    "    df_scores['health_score'] = (df_scores['green'] + df_scores['moisture_high']) / 2\n",
    "    df_scores['stress_score'] = (df_scores['dead'] + df_scores['moisture_low']) / 2\n",
    "    df_scores['texture_balance'] = df_scores['texture_fine'] / (df_scores['texture_coarse'] + 1e-6)\n",
    "    \n",
    "    return df_scores.values\n",
    "\n",
    "class SupervisedEmbeddingEngine(BaseEstimator, TransformerMixin):\n",
    "    def __init__(self, n_pca=0.98, n_pls=8, n_gmm=5, random_state=42):\n",
    "        self.n_pca = n_pca\n",
    "        self.n_pls = n_pls\n",
    "        self.n_gmm = n_gmm\n",
    "        self.random_state = random_state\n",
    "        self.scaler = StandardScaler()\n",
    "        self.pca = PCA(n_components=n_pca, random_state=random_state)\n",
    "        self.pls = PLSRegression(n_components=n_pls, scale=False)\n",
    "        self.gmm = GaussianMixture(n_components=n_gmm, covariance_type='diag', random_state=random_state)\n",
    "        self.pls_fitted_ = False\n",
    "\n",
    "    def fit(self, X, y=None, X_semantic=None):\n",
    "        X_scaled = self.scaler.fit_transform(X)\n",
    "        self.pca.fit(X_scaled)\n",
    "        self.gmm.fit(X_scaled)\n",
    "        if y is not None:\n",
    "            y_clean = y.values if hasattr(y, 'values') else y\n",
    "            self.pls.fit(X_scaled, y_clean)\n",
    "            self.pls_fitted_ = True\n",
    "        return self\n",
    "\n",
    "    def transform(self, X, X_semantic=None):\n",
    "        X_scaled = self.scaler.transform(X)\n",
    "        return self._generate_features(X_scaled, X_semantic)\n",
    "\n",
    "    def _generate_features(self, X_scaled, X_semantic=None):\n",
    "        features = []\n",
    "        \n",
    "        # PCA features\n",
    "        f_pca = self.pca.transform(X_scaled)\n",
    "        features.append(f_pca)\n",
    "        \n",
    "        # PLS features\n",
    "        if self.pls_fitted_:\n",
    "            f_pls = self.pls.transform(X_scaled)\n",
    "            features.append(f_pls)\n",
    "        \n",
    "        # GMM features\n",
    "        f_gmm = self.gmm.predict_proba(X_scaled)\n",
    "        features.append(f_gmm)\n",
    "        \n",
    "        # Semantic features\n",
    "        if X_semantic is not None:\n",
    "            sem_norm = (X_semantic - np.mean(X_semantic, axis=0)) / (np.std(X_semantic, axis=0) + 1e-6)\n",
    "            features.append(sem_norm)\n",
    "        \n",
    "        # Statistical features from raw embeddings\n",
    "        f_stats = np.column_stack([\n",
    "            X_scaled.mean(axis=1),\n",
    "            X_scaled.std(axis=1),\n",
    "            X_scaled.max(axis=1),\n",
    "            X_scaled.min(axis=1),\n",
    "        ])\n",
    "        features.append(f_stats)\n",
    "        \n",
    "        return np.hstack(features)\n",
    "\n",
    "def compare_results(oof, train_data):\n",
    "    y_oof_df = pd.DataFrame(oof, columns=TARGET_NAMES)\n",
    "    raw_score = competition_metric(train_data[TARGET_NAMES].values, y_oof_df.values)\n",
    "    print(f\"Raw CV Score: {raw_score:.6f}\")\n",
    "    \n",
    "    # Try different post-processing methods\n",
    "    for method in ['hierarchical', 'simple', 'weighted']:\n",
    "        y_oof_proc = post_process_biomass(y_oof_df, method=method)\n",
    "        proc_score = competition_metric(train_data[TARGET_NAMES].values, y_oof_proc.values)\n",
    "        print(f\"{method.capitalize()} CV Score: {proc_score:.6f} (Δ: {proc_score - raw_score:+.6f})\")\n",
    "\n",
    "def cross_validate(model, train_data, test_data, feature_engine, semantic_train=None, semantic_test=None, target_transform='max', seed=42):\n",
    "    n_splits = train_data['fold'].nunique()\n",
    "    target_max_arr = np.array([TARGET_MAX[t] for t in TARGET_NAMES], dtype=float)\n",
    "    y_true = train_data[TARGET_NAMES]\n",
    "    y_pred = pd.DataFrame(0.0, index=train_data.index, columns=TARGET_NAMES)\n",
    "    y_pred_test = np.zeros([len(test_data), len(TARGET_NAMES)], dtype=float)\n",
    "    \n",
    "    COLUMNS = [col for col in train_data.columns if col.startswith('emb')]\n",
    "    \n",
    "    for fold in range(n_splits):\n",
    "        seeding(seed*(seed//2 + fold))\n",
    "        train_mask = train_data['fold'] != fold\n",
    "        valid_mask = train_data['fold'] == fold\n",
    "        val_idx = train_data[valid_mask].index\n",
    "        \n",
    "        X_train_raw = train_data[train_mask][COLUMNS].values\n",
    "        X_valid_raw = train_data[valid_mask][COLUMNS].values\n",
    "        X_test_raw = test_data[COLUMNS].values\n",
    "        \n",
    "        sem_train_fold = semantic_train[train_mask] if semantic_train is not None else None\n",
    "        sem_valid_fold = semantic_train[valid_mask] if semantic_train is not None else None\n",
    "        \n",
    "        y_train = train_data[train_mask][TARGET_NAMES].values\n",
    "        y_valid = train_data[valid_mask][TARGET_NAMES].values\n",
    "        \n",
    "        if target_transform == 'log':\n",
    "            y_train_proc = np.log1p(y_train)\n",
    "        elif target_transform == 'max':\n",
    "            y_train_proc = y_train / target_max_arr\n",
    "        elif target_transform == 'sqrt':\n",
    "            y_train_proc = np.sqrt(y_train)\n",
    "        else:\n",
    "            y_train_proc = y_train\n",
    "        \n",
    "        engine = deepcopy(feature_engine)\n",
    "        engine.fit(X_train_raw, y=y_train_proc, X_semantic=sem_train_fold)\n",
    "        \n",
    "        x_train_eng = engine.transform(X_train_raw, X_semantic=sem_train_fold)\n",
    "        x_valid_eng = engine.transform(X_valid_raw, X_semantic=sem_valid_fold)\n",
    "        x_test_eng = engine.transform(X_test_raw, X_semantic=semantic_test)\n",
    "        \n",
    "        fold_valid_pred = np.zeros_like(y_valid)\n",
    "        fold_test_pred = np.zeros([len(test_data), len(TARGET_NAMES)])\n",
    "        \n",
    "        for k in range(len(TARGET_NAMES)):\n",
    "            regr = deepcopy(model)\n",
    "            regr.fit(x_train_eng, y_train_proc[:, k])\n",
    "            \n",
    "            pred_valid_raw = regr.predict(x_valid_eng)\n",
    "            pred_test_raw = regr.predict(x_test_eng)\n",
    "            \n",
    "            if target_transform == 'log':\n",
    "                pred_valid_inv = np.expm1(pred_valid_raw)\n",
    "                pred_test_inv = np.expm1(pred_test_raw)\n",
    "            elif target_transform == 'max':\n",
    "                pred_valid_inv = (pred_valid_raw * target_max_arr[k])\n",
    "                pred_test_inv = (pred_test_raw * target_max_arr[k])\n",
    "            elif target_transform == 'sqrt':\n",
    "                pred_valid_inv = pred_valid_raw ** 2\n",
    "                pred_test_inv = pred_test_raw ** 2\n",
    "            else:\n",
    "                pred_valid_inv = pred_valid_raw\n",
    "                pred_test_inv = pred_test_raw\n",
    "            \n",
    "            fold_valid_pred[:, k] = pred_valid_inv\n",
    "            fold_test_pred[:, k] = pred_test_inv\n",
    "        \n",
    "        y_pred.loc[val_idx] = fold_valid_pred\n",
    "        y_pred_test += fold_test_pred / n_splits\n",
    "    \n",
    "    full_cv = competition_metric(y_true.values, y_pred.values)\n",
    "    print(f\"Full CV Score: {full_cv:.6f}\")\n",
    "    return y_pred.values, y_pred_test\n",
    "\n",
    "# ============================================================================\n",
    "# MAIN EXECUTION WITH 5 SUBMISSIONS\n",
    "# ============================================================================\n",
    "def create_submission_1(train_df, test_df, timeout_mgr):\n",
    "    \"\"\"\n",
    "    Submission 1: LightGBM + CatBoost + XGBoost Ensemble with Hierarchical Post-processing\n",
    "    \"\"\"\n",
    "    print(\"\\n\" + \"=\"*80)\n",
    "    print(\"SUBMISSION 1: LightGBM + CatBoost + XGBoost Ensemble\")\n",
    "    print(\"=\"*80)\n",
    "    \n",
    "    try:\n",
    "        # Load or compute embeddings\n",
    "        train_embeddings_path = \"/kaggle/input/csiro-datasplit/csiro_data_split.csv\"\n",
    "        if os.path.exists(train_embeddings_path):\n",
    "            train_siglip_df = pd.read_csv(train_embeddings_path)\n",
    "            print(\"Loaded pre-computed training embeddings\")\n",
    "        else:\n",
    "            siglip_path = \"/kaggle/input/google-siglip-so400m-patch14-384/transformers/default/1\"\n",
    "            train_siglip_df = compute_embeddings(model_path=siglip_path, df=train_df, patch_size=520)\n",
    "        \n",
    "        siglip_path = \"/kaggle/input/google-siglip-so400m-patch14-384/transformers/default/1\"\n",
    "        test_siglip_df = compute_embeddings(model_path=siglip_path, df=test_df, patch_size=520)\n",
    "        \n",
    "        # Generate semantic features\n",
    "        X_all_emb = np.vstack([train_siglip_df.filter(like=\"emb\").values, test_siglip_df.filter(like=\"emb\").values])\n",
    "        try:\n",
    "            all_semantic_scores = generate_semantic_features(X_all_emb, model_path=siglip_path)\n",
    "            n_train = len(train_siglip_df)\n",
    "            sem_train_full = all_semantic_scores[:n_train]\n",
    "            sem_test_full = all_semantic_scores[n_train:]\n",
    "        except:\n",
    "            sem_train_full = None\n",
    "            sem_test_full = None\n",
    "        \n",
    "        feat_engine = SupervisedEmbeddingEngine(n_pca=0.85, n_pls=10, n_gmm=8)\n",
    "        \n",
    "        # Model 1: LightGBM\n",
    "        print(\"\\nTraining LightGBM...\")\n",
    "        oof_lgbm, pred_lgbm = cross_validate(\n",
    "            LGBMRegressor(n_estimators=200, learning_rate=0.05, max_depth=8, verbose=-1),\n",
    "            train_siglip_df, test_siglip_df, \n",
    "            feature_engine=feat_engine, \n",
    "            semantic_train=sem_train_full, \n",
    "            semantic_test=sem_test_full, \n",
    "            target_transform='max'\n",
    "        )\n",
    "        \n",
    "        # Model 2: CatBoost\n",
    "        print(\"\\nTraining CatBoost...\")\n",
    "        oof_cat, pred_cat = cross_validate(\n",
    "            CatBoostRegressor(iterations=200, learning_rate=0.05, depth=8, verbose=0),\n",
    "            train_siglip_df, test_siglip_df, \n",
    "            feature_engine=feat_engine, \n",
    "            semantic_train=sem_train_full, \n",
    "            semantic_test=sem_test_full,\n",
    "            target_transform='max'\n",
    "        )\n",
    "        \n",
    "        # Model 3: XGBoost\n",
    "        print(\"\\nTraining XGBoost...\")\n",
    "        oof_xgb, pred_xgb = cross_validate(\n",
    "            XGBRegressor(n_estimators=200, learning_rate=0.05, max_depth=8, verbosity=0),\n",
    "            train_siglip_df, test_siglip_df, \n",
    "            feature_engine=feat_engine, \n",
    "            semantic_train=sem_train_full, \n",
    "            semantic_test=sem_test_full,\n",
    "            target_transform='max'\n",
    "        )\n",
    "        \n",
    "        # Weighted ensemble\n",
    "        pred_test = 0.4 * pred_lgbm + 0.35 * pred_cat + 0.25 * pred_xgb\n",
    "        \n",
    "        # Post-processing\n",
    "        test_df[TARGET_NAMES] = pred_test\n",
    "        test_df = post_process_biomass(test_df, method='hierarchical')\n",
    "        \n",
    "        # Save\n",
    "        sub_df = melt_table(test_df)\n",
    "        sub_df[['sample_id', 'target']].to_csv(\"submission1.csv\", index=False)\n",
    "        \n",
    "        print(\"✓ Submission 1 created successfully\")\n",
    "        timeout_mgr.log_time(\"Completed Submission 1\")\n",
    "        return True\n",
    "        \n",
    "    except Exception as e:\n",
    "        print(f\"✗ Submission 1 failed: {e}\")\n",
    "        return False\n",
    "\n",
    "def create_submission_2(train_df, test_df, timeout_mgr):\n",
    "    \"\"\"\n",
    "    Submission 2: Diverse Model Ensemble with Simple Post-processing\n",
    "    \"\"\"\n",
    "    print(\"\\n\" + \"=\"*80)\n",
    "    print(\"SUBMISSION 2: Diverse Model Ensemble\")\n",
    "    print(\"=\"*80)\n",
    "    \n",
    "    try:\n",
    "        # Load embeddings\n",
    "        train_embeddings_path = \"/kaggle/input/csiro-datasplit/csiro_data_split.csv\"\n",
    "        if os.path.exists(train_embeddings_path):\n",
    "            train_siglip_df = pd.read_csv(train_embeddings_path)\n",
    "        else:\n",
    "            siglip_path = \"/kaggle/input/google-siglip-so400m-patch14-384/transformers/default/1\"\n",
    "            train_siglip_df = compute_embeddings(model_path=siglip_path, df=train_df, patch_size=520)\n",
    "        \n",
    "        siglip_path = \"/kaggle/input/google-siglip-so400m-patch14-384/transformers/default/1\"\n",
    "        test_siglip_df = compute_embeddings(model_path=siglip_path, df=test_df, patch_size=520)\n",
    "        \n",
    "        # Generate semantic features\n",
    "        X_all_emb = np.vstack([train_siglip_df.filter(like=\"emb\").values, test_siglip_df.filter(like=\"emb\").values])\n",
    "        try:\n",
    "            all_semantic_scores = generate_semantic_features(X_all_emb, model_path=siglip_path)\n",
    "            n_train = len(train_siglip_df)\n",
    "            sem_train_full = all_semantic_scores[:n_train]\n",
    "            sem_test_full = all_semantic_scores[n_train:]\n",
    "        except:\n",
    "            sem_train_full = None\n",
    "            sem_test_full = None\n",
    "        \n",
    "        feat_engine = SupervisedEmbeddingEngine(n_pca=0.90, n_pls=12, n_gmm=6)\n",
    "        \n",
    "        # Model 1: Gradient Boosting\n",
    "        print(\"\\nTraining Gradient Boosting...\")\n",
    "        oof_gb, pred_gb = cross_validate(\n",
    "            GradientBoostingRegressor(n_estimators=150, learning_rate=0.1, max_depth=6),\n",
    "            train_siglip_df, test_siglip_df, \n",
    "            feature_engine=feat_engine, \n",
    "            semantic_train=sem_train_full, \n",
    "            semantic_test=sem_test_full,\n",
    "            target_transform='log'\n",
    "        )\n",
    "        \n",
    "        # Model 2: Extra Trees\n",
    "        print(\"\\nTraining Extra Trees...\")\n",
    "        oof_et, pred_et = cross_validate(\n",
    "            ExtraTreesRegressor(n_estimators=100, max_depth=15, random_state=42),\n",
    "            train_siglip_df, test_siglip_df, \n",
    "            feature_engine=feat_engine, \n",
    "            semantic_train=sem_train_full, \n",
    "            semantic_test=sem_test_full,\n",
    "            target_transform='sqrt'\n",
    "        )\n",
    "        \n",
    "        # Model 3: Random Forest\n",
    "        print(\"\\nTraining Random Forest...\")\n",
    "        oof_rf, pred_rf = cross_validate(\n",
    "            RandomForestRegressor(n_estimators=100, max_depth=15, random_state=42),\n",
    "            train_siglip_df, test_siglip_df, \n",
    "            feature_engine=feat_engine, \n",
    "            semantic_train=sem_train_full, \n",
    "            semantic_test=sem_test_full,\n",
    "            target_transform='max'\n",
    "        )\n",
    "        \n",
    "        # Ensemble\n",
    "        pred_test = 0.4 * pred_gb + 0.35 * pred_et + 0.25 * pred_rf\n",
    "        \n",
    "        # Post-processing\n",
    "        test_df[TARGET_NAMES] = pred_test\n",
    "        test_df = post_process_biomass(test_df, method='simple')\n",
    "        \n",
    "        # Save\n",
    "        sub_df = melt_table(test_df)\n",
    "        sub_df[['sample_id', 'target']].to_csv(\"submission2.csv\", index=False)\n",
    "        \n",
    "        print(\"✓ Submission 2 created successfully\")\n",
    "        timeout_mgr.log_time(\"Completed Submission 2\")\n",
    "        return True\n",
    "        \n",
    "    except Exception as e:\n",
    "        print(f\"✗ Submission 2 failed: {e}\")\n",
    "        return False\n",
    "\n",
    "def create_submission_3(train_df, test_df, timeout_mgr):\n",
    "    \"\"\"\n",
    "    Submission 3: Conservative Ensemble with Weighted Post-processing\n",
    "    \"\"\"\n",
    "    print(\"\\n\" + \"=\"*80)\n",
    "    print(\"SUBMISSION 3: Conservative Ensemble\")\n",
    "    print(\"=\"*80)\n",
    "    \n",
    "    try:\n",
    "        # Load embeddings\n",
    "        train_embeddings_path = \"/kaggle/input/csiro-datasplit/csiro_data_split.csv\"\n",
    "        if os.path.exists(train_embeddings_path):\n",
    "            train_siglip_df = pd.read_csv(train_embeddings_path)\n",
    "        else:\n",
    "            siglip_path = \"/kaggle/input/google-siglip-so400m-patch14-384/transformers/default/1\"\n",
    "            train_siglip_df = compute_embeddings(model_path=siglip_path, df=train_df, patch_size=520)\n",
    "        \n",
    "        siglip_path = \"/kaggle/input/google-siglip-so400m-patch14-384/transformers/default/1\"\n",
    "        test_siglip_df = compute_embeddings(model_path=siglip_path, df=test_df, patch_size=520)\n",
    "        \n",
    "        # Generate semantic features\n",
    "        X_all_emb = np.vstack([train_siglip_df.filter(like=\"emb\").values, test_siglip_df.filter(like=\"emb\").values])\n",
    "        try:\n",
    "            all_semantic_scores = generate_semantic_features(X_all_emb, model_path=siglip_path)\n",
    "            n_train = len(train_siglip_df)\n",
    "            sem_train_full = all_semantic_scores[:n_train]\n",
    "            sem_test_full = all_semantic_scores[n_train:]\n",
    "        except:\n",
    "            sem_train_full = None\n",
    "            sem_test_full = None\n",
    "        \n",
    "        feat_engine = SupervisedEmbeddingEngine(n_pca=0.95, n_pls=6, n_gmm=4)\n",
    "        \n",
    "        # Model 1: Ridge Regression\n",
    "        print(\"\\nTraining Ridge...\")\n",
    "        oof_ridge, pred_ridge = cross_validate(\n",
    "            Ridge(alpha=1.0),\n",
    "            train_siglip_df, test_siglip_df, \n",
    "            feature_engine=feat_engine, \n",
    "            semantic_train=sem_train_full, \n",
    "            semantic_test=sem_test_full,\n",
    "            target_transform='max'\n",
    "        )\n",
    "        \n",
    "        # Model 2: Bayesian Ridge\n",
    "        print(\"\\nTraining Bayesian Ridge...\")\n",
    "        oof_br, pred_br = cross_validate(\n",
    "            BayesianRidge(),\n",
    "            train_siglip_df, test_siglip_df, \n",
    "            feature_engine=feat_engine, \n",
    "            semantic_train=sem_train_full, \n",
    "            semantic_test=sem_test_full,\n",
    "            target_transform='max'\n",
    "        )\n",
    "        \n",
    "        # Model 3: LightGBM (conservative)\n",
    "        print(\"\\nTraining LightGBM (conservative)...\")\n",
    "        oof_lgbm, pred_lgbm = cross_validate(\n",
    "            LGBMRegressor(n_estimators=100, learning_rate=0.03, max_depth=6, verbose=-1),\n",
    "            train_siglip_df, test_siglip_df, \n",
    "            feature_engine=feat_engine, \n",
    "            semantic_train=sem_train_full, \n",
    "            semantic_test=sem_test_full,\n",
    "            target_transform='max'\n",
    "        )\n",
    "        \n",
    "        # Ensemble\n",
    "        pred_test = 0.25 * pred_ridge + 0.25 * pred_br + 0.5 * pred_lgbm\n",
    "        \n",
    "        # Post-processing\n",
    "        test_df[TARGET_NAMES] = pred_test\n",
    "        test_df = post_process_biomass(test_df, method='weighted')\n",
    "        \n",
    "        # Save\n",
    "        sub_df = melt_table(test_df)\n",
    "        sub_df[['sample_id', 'target']].to_csv(\"submission3.csv\", index=False)\n",
    "        \n",
    "        print(\"✓ Submission 3 created successfully\")\n",
    "        timeout_mgr.log_time(\"Completed Submission 3\")\n",
    "        return True\n",
    "        \n",
    "    except Exception as e:\n",
    "        print(f\"✗ Submission 3 failed: {e}\")\n",
    "        return False\n",
    "\n",
    "def create_submission_4(train_df, test_df, timeout_mgr):\n",
    "    \"\"\"\n",
    "    Submission 4: Aggressive Boosting Ensemble\n",
    "    \"\"\"\n",
    "    print(\"\\n\" + \"=\"*80)\n",
    "    print(\"SUBMISSION 4: Aggressive Boosting Ensemble\")\n",
    "    print(\"=\"*80)\n",
    "    \n",
    "    try:\n",
    "        # Load embeddings\n",
    "        train_embeddings_path = \"/kaggle/input/csiro-datasplit/csiro_data_split.csv\"\n",
    "        if os.path.exists(train_embeddings_path):\n",
    "            train_siglip_df = pd.read_csv(train_embeddings_path)\n",
    "        else:\n",
    "            siglip_path = \"/kaggle/input/google-siglip-so400m-patch14-384/transformers/default/1\"\n",
    "            train_siglip_df = compute_embeddings(model_path=siglip_path, df=train_df, patch_size=520)\n",
    "        \n",
    "        siglip_path = \"/kaggle/input/google-siglip-so400m-patch14-384/transformers/default/1\"\n",
    "        test_siglip_df = compute_embeddings(model_path=siglip_path, df=test_df, patch_size=520)\n",
    "        \n",
    "        # Generate semantic features\n",
    "        X_all_emb = np.vstack([train_siglip_df.filter(like=\"emb\").values, test_siglip_df.filter(like=\"emb\").values])\n",
    "        try:\n",
    "            all_semantic_scores = generate_semantic_features(X_all_emb, model_path=siglip_path)\n",
    "            n_train = len(train_siglip_df)\n",
    "            sem_train_full = all_semantic_scores[:n_train]\n",
    "            sem_test_full = all_semantic_scores[n_train:]\n",
    "        except:\n",
    "            sem_train_full = None\n",
    "            sem_test_full = None\n",
    "        \n",
    "        feat_engine = SupervisedEmbeddingEngine(n_pca=0.88, n_pls=15, n_gmm=10)\n",
    "        \n",
    "        # Model 1: XGBoost (aggressive)\n",
    "        print(\"\\nTraining XGBoost (aggressive)...\")\n",
    "        oof_xgb, pred_xgb = cross_validate(\n",
    "            XGBRegressor(n_estimators=300, learning_rate=0.1, max_depth=10, verbosity=0),\n",
    "            train_siglip_df, test_siglip_df, \n",
    "            feature_engine=feat_engine, \n",
    "            semantic_train=sem_train_full, \n",
    "            semantic_test=sem_test_full,\n",
    "            target_transform='log'\n",
    "        )\n",
    "        \n",
    "        # Model 2: LightGBM (aggressive)\n",
    "        print(\"\\nTraining LightGBM (aggressive)...\")\n",
    "        oof_lgbm, pred_lgbm = cross_validate(\n",
    "            LGBMRegressor(n_estimators=300, learning_rate=0.1, max_depth=10, verbose=-1),\n",
    "            train_siglip_df, test_siglip_df, \n",
    "            feature_engine=feat_engine, \n",
    "            semantic_train=sem_train_full, \n",
    "            semantic_test=sem_test_full,\n",
    "            target_transform='log'\n",
    "        )\n",
    "        \n",
    "        # Model 3: CatBoost (aggressive)\n",
    "        print(\"\\nTraining CatBoost (aggressive)...\")\n",
    "        oof_cat, pred_cat = cross_validate(\n",
    "            CatBoostRegressor(iterations=300, learning_rate=0.1, depth=10, verbose=0),\n",
    "            train_siglip_df, test_siglip_df, \n",
    "            feature_engine=feat_engine, \n",
    "            semantic_train=sem_train_full, \n",
    "            semantic_test=sem_test_full,\n",
    "            target_transform='log'\n",
    "        )\n",
    "        \n",
    "        # Ensemble\n",
    "        pred_test = 0.35 * pred_xgb + 0.35 * pred_lgbm + 0.3 * pred_cat\n",
    "        \n",
    "        # Post-processing\n",
    "        test_df[TARGET_NAMES] = pred_test\n",
    "        test_df = post_process_biomass(test_df, method='hierarchical')\n",
    "        \n",
    "        # Save\n",
    "        sub_df = melt_table(test_df)\n",
    "        sub_df[['sample_id', 'target']].to_csv(\"submission4.csv\", index=False)\n",
    "        \n",
    "        print(\"✓ Submission 4 created successfully\")\n",
    "        timeout_mgr.log_time(\"Completed Submission 4\")\n",
    "        return True\n",
    "        \n",
    "    except Exception as e:\n",
    "        print(f\"✗ Submission 4 failed: {e}\")\n",
    "        return False\n",
    "\n",
    "def create_submission_5(timeout_mgr):\n",
    "    \"\"\"\n",
    "    Submission 5: Meta-Ensemble of all previous submissions\n",
    "    \"\"\"\n",
    "    print(\"\\n\" + \"=\"*80)\n",
    "    print(\"SUBMISSION 5: Meta-Ensemble\")\n",
    "    print(\"=\"*80)\n",
    "    \n",
    "    try:\n",
    "        # Load all submissions\n",
    "        submissions = []\n",
    "        weights = []\n",
    "        \n",
    "        for i in range(1, 5):\n",
    "            filename = f\"submission{i}.csv\"\n",
    "            if os.path.exists(filename):\n",
    "                sub = pd.read_csv(filename)\n",
    "                submissions.append(sub)\n",
    "                # Weights based on expected performance\n",
    "                if i == 1:\n",
    "                    weights.append(0.35)  # LightGBM+CatBoost+XGBoost is typically strong\n",
    "                elif i == 2:\n",
    "                    weights.append(0.25)  # Diverse ensemble\n",
    "                elif i == 3:\n",
    "                    weights.append(0.20)  # Conservative\n",
    "                else:\n",
    "                    weights.append(0.20)  # Aggressive\n",
    "        \n",
    "        if len(submissions) == 0:\n",
    "            print(\"No submissions found for meta-ensemble\")\n",
    "            return False\n",
    "        \n",
    "        # Normalize weights\n",
    "        weights = np.array(weights) / sum(weights)\n",
    "        \n",
    "        # Create weighted ensemble\n",
    "        meta_sub = submissions[0].copy()\n",
    "        meta_sub['target'] = 0\n",
    "        \n",
    "        for sub, weight in zip(submissions, weights):\n",
    "            meta_sub['target'] += sub['target'] * weight\n",
    "        \n",
    "        # Apply additional smoothing\n",
    "        meta_sub['target'] = meta_sub['target'].clip(0.01, 400.0)\n",
    "        \n",
    "        # Save\n",
    "        meta_sub.to_csv(\"submission5.csv\", index=False)\n",
    "        \n",
    "        print(\"✓ Submission 5 created successfully\")\n",
    "        timeout_mgr.log_time(\"Completed Submission 5\")\n",
    "        return True\n",
    "        \n",
    "    except Exception as e:\n",
    "        print(f\"✗ Submission 5 failed: {e}\")\n",
    "        return False\n",
    "\n",
    "def select_best_submission():\n",
    "    \"\"\"\n",
    "    Select the best submission as the final submission\n",
    "    \"\"\"\n",
    "    print(\"\\n\" + \"=\"*80)\n",
    "    print(\"SELECTING BEST SUBMISSION\")\n",
    "    print(\"=\"*80)\n",
    "    \n",
    "    # Priority order (based on typical performance)\n",
    "    priority = ['submission1.csv', 'submission5.csv', 'submission4.csv', 'submission3.csv', 'submission2.csv']\n",
    "    \n",
    "    for filename in priority:\n",
    "        if os.path.exists(filename):\n",
    "            shutil.copy(filename, 'submission.csv')\n",
    "            print(f\"✓ Selected {filename} as final submission\")\n",
    "            return True\n",
    "    \n",
    "    # Fallback to sample submission\n",
    "    try:\n",
    "        sample_path = \"/kaggle/input/csiro-biomass/sample_submission.csv\"\n",
    "        if os.path.exists(sample_path):\n",
    "            shutil.copy(sample_path, 'submission.csv')\n",
    "            print(\"⚠ Using sample submission as fallback\")\n",
    "            return True\n",
    "    except:\n",
    "        pass\n",
    "    \n",
    "    print(\"✗ Could not create any submission\")\n",
    "    return False\n",
    "\n",
    "def main():\n",
    "    \"\"\"\n",
    "    Main execution function with timeout management\n",
    "    \"\"\"\n",
    "    # Initialize timeout manager (8.5 hours for Kaggle competitions)\n",
    "    timeout_mgr = TimeoutManager(max_time_seconds=8.5 * 3600)\n",
    "    \n",
    "    print(\"=\"*80)\n",
    "    print(\"CSIRO BIOMASS PREDICTION - COMPREHENSIVE ENSEMBLE\")\n",
    "    print(\"=\"*80)\n",
    "    \n",
    "    seeding(42)\n",
    "    timeout_mgr.log_time(\"Started execution\")\n",
    "    \n",
    "    # Create fallback\n",
    "    try:\n",
    "        sample_sub_path = cfg.DATA_PATH / 'sample_submission.csv'\n",
    "        if os.path.exists(sample_sub_path):\n",
    "            sample_sub = pd.read_csv(sample_sub_path)\n",
    "            sample_sub.to_csv('submission.csv', index=False)\n",
    "            print(\"Created fallback submission\")\n",
    "    except Exception as e:\n",
    "        print(f\"Warning: Could not create fallback: {e}\")\n",
    "    \n",
    "    # Load data\n",
    "    try:\n",
    "        test_df = pd.read_csv(cfg.DATA_PATH/'test.csv')\n",
    "        test_df = pivot_table(df=test_df)\n",
    "        test_df['image_path'] = test_df['image_path'].apply(lambda p: str(cfg.DATA_PATH / p))\n",
    "        \n",
    "        train_df = pd.read_csv(cfg.DATA_PATH/'train.csv')\n",
    "        train_df = pivot_table(df=train_df)\n",
    "        train_df['image_path'] = train_df['image_path'].apply(lambda p: str(cfg.DATA_PATH / p))\n",
    "        \n",
    "        timeout_mgr.log_time(\"Loaded data\")\n",
    "    except Exception as e:\n",
    "        print(f\"✗ Data loading failed: {e}\")\n",
    "        return\n",
    "    \n",
    "    # Create submissions\n",
    "    success_count = 0\n",
    "    \n",
    "    # Submission 1 (Priority: Highest)\n",
    "    if timeout_mgr.should_continue(buffer_seconds=6*3600):\n",
    "        if create_submission_1(train_df, test_df, timeout_mgr):\n",
    "            success_count += 1\n",
    "    \n",
    "    # Submission 2\n",
    "    if timeout_mgr.should_continue(buffer_seconds=5*3600):\n",
    "        if create_submission_2(train_df, test_df, timeout_mgr):\n",
    "            success_count += 1\n",
    "    \n",
    "    # Submission 3\n",
    "    if timeout_mgr.should_continue(buffer_seconds=4*3600):\n",
    "        if create_submission_3(train_df, test_df, timeout_mgr):\n",
    "            success_count += 1\n",
    "    \n",
    "    # Submission 4\n",
    "    if timeout_mgr.should_continue(buffer_seconds=3*3600):\n",
    "        if create_submission_4(train_df, test_df, timeout_mgr):\n",
    "            success_count += 1\n",
    "    \n",
    "    # Submission 5 (Meta-ensemble)\n",
    "    if timeout_mgr.should_continue(buffer_seconds=1*3600):\n",
    "        if create_submission_5(timeout_mgr):\n",
    "            success_count += 1\n",
    "    \n",
    "    print(f\"\\n✓ Successfully created {success_count} submissions\")\n",
    "    \n",
    "    # Select best submission\n",
    "    select_best_submission()\n",
    "    \n",
    "    # Print summary\n",
    "    timeout_mgr.get_summary()\n",
    "    \n",
    "    print(\"\\n\" + \"=\"*80)\n",
    "    print(\"EXECUTION COMPLETE\")\n",
    "    print(\"=\"*80)\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    try:\n",
    "        main()\n",
    "    except Exception as e:\n",
    "        print(f\"CRITICAL ERROR: {e}\")\n",
    "        import traceback\n",
    "        traceback.print_exc()\n",
    "        \n",
    "        # Emergency fallback\n",
    "        try:\n",
    "            sample_path = \"/kaggle/input/csiro-biomass/sample_submission.csv\"\n",
    "            if os.path.exists(sample_path):\n",
    "                sample = pd.read_csv(sample_path)\n",
    "                sample.to_csv('submission.csv', index=False)\n",
    "                print(\"Created emergency fallback submission\")\n",
    "        except:\n",
    "            print(\"Could not create emergency fallback\")"
   ]
  }
